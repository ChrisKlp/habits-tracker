{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalSetup": "<rootDir>/test/config/global-setup.mjs",
  "globalTeardown": "<rootDir>/test/config/global-teardown.mjs",
  "testTimeout": 30000,
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}